out = outfile("~/Desktop/cordicR3.out" "w")
outputs()

for(tt 2 20
	time = tt*220p	
	fprintf(out  "cos = %d"  round(value(VT("/a2<8>")  time)))
	fprintf(out  "%d"  round(value(VT("/a2<7>")  time)))
	fprintf(out  "%d"  round(value(VT("/a2<6>")  time)))
	fprintf(out  "%d"  round(value(VT("/a2<5>")  time)))
	fprintf(out  "%d"  round(value(VT("/a2<4>")  time)))
	fprintf(out  "%d"  round(value(VT("/a2<3>")  time)))
	fprintf(out  "%d"  round(value(VT("/a2<2>")  time)))
	fprintf(out  "%d"  round(value(VT("/a2<1>")  time)))
	fprintf(out  "%d  "  round(value(VT("/a2<0>")  time)))

	fprintf(out  "sin = %d"  round(value(VT("/b2<8>")  time)))
	fprintf(out  "%d"  round(value(VT("/b2<7>")  time)))
	fprintf(out  "%d"  round(value(VT("/b2<6>")  time)))
	fprintf(out  "%d"  round(value(VT("/b2<5>")  time)))
	fprintf(out  "%d"  round(value(VT("/b2<4>")  time)))
	fprintf(out  "%d"  round(value(VT("/b2<3>")  time)))
	fprintf(out  "%d"  round(value(VT("/b2<2>")  time)))
	fprintf(out  "%d"  round(value(VT("/b2<1>")  time)))
	fprintf(out  "%d\n"  round(value(VT("/b2<0>")  time)))
)

close(out)
